<!DOCTYPE html>
<html lang="fr" class="no-js demo-4">

    <head>
        <meta charset="UTF-8" />

        <!--Let browser know website is optimized for mobile-->
        <meta name="viewport" content="width=device-width, initial-scale=1.0"/>

        <title>{% block title %}Florence Menet-Pélisson{% endblock %}</title>

        {% block stylesheets %}
            <!-- Compiled and minified CSS -->
            <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.7/css/materialize.min.css">

            <!-- Icônes -->
            <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
            <link rel="stylesheet" type="text/css" href="{{ asset('bundles/flo/fonts/font-awesome-4.3.0/css/font-awesome.min.css') }}" />

            <!-- Appel CSS personnalisé -->
            <link rel="stylesheet" type="text/css" href="{{ asset('bundles/flo/css/style.css') }}"/>

            <script src="{{ asset('bundles/flo/js/modernizr-custom.js') }}"></script>

        {% endblock %}

        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

    </head>

    {% block header %}
    {% endblock %}

    <body>

        {% block body %}{% endblock %}

        <footer>
            <div class="footer-copyright">

                    © 2016 Copyright

            </div>
        </footer>

        {% block javascripts %}
            <script src="https://code.jquery.com/jquery-2.1.3.min.js"></script>
            <!-- Compiled and minified JavaScript -->
            <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.7/js/materialize.min.js"></script>
            <script src="{{ asset('bundles/flo/js/imagesloaded.pkgd.min.js') }}"></script>
            <script src="{{ asset('bundles/flo/js/masonry.pkgd.min.js') }}"></script>
            <script src="{{ asset('bundles/flo/js/classie.js') }}"></script>
            <script src="{{ asset('bundles/flo/js/main.js') }}"></script>
            <script src="{{ asset('bundles/flo/js/flo.js') }}"</script
        {% endblock %}


    </body>


</html>
